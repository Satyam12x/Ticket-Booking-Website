{
  "version": 2,
  "builds": [
    {
      "src": "cinema-frontend/next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "cinema-backend/src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "cinema-backend/src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "cinema-frontend/$1"
    }
  ]
}
